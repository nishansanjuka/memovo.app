{
  "name": "memovo.app",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "ci": "turbo run lint check-types build test",
    "docker:build": "docker compose -f docker-compose.deploy.yml build",
    "docker:push": "docker compose -f docker-compose.deploy.yml push",
    "docker:up": "docker compose -f docker-compose.deploy.yml up",
    "sync:postman": "turbo run sync:postman",
    "sync:postman:env": "turbo run sync:postman:env",
    "sync:postman:all": "turbo run sync:postman:all"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.12"
    }
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
